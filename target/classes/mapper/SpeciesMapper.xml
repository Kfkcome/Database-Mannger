<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="swinedb.mapper.SpeciesMapper">

    <resultMap id="BaseResultMap" type="swinedb.domain.Species">
            <id property="speciesId" column="species_id" jdbcType="INTEGER"/>
            <result property="speciesName" column="species_name" jdbcType="VARCHAR"/>
            <result property="genomeId" column="genome_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        species_id,species_name,genome_id
    </sql>
</mapper>

            <result property="endPoint" column="END_POINT" jdbcType="BIGINT"/>
            <result property="direction" column="DIRECTION" jdbcType="INTEGER"/>
            <result property="tissueId" column="TISSUE_ID" jdbcType="INTEGER"/>

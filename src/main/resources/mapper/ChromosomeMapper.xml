<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="swine_search.mapper.ChromosomeMapper">

    <resultMap id="BaseResultMap" type="swine_search.domain.Chromosome">
            <id property="chrId" column="chr_id" jdbcType="INTEGER"/>
            <result property="chrName" column="chr_name" jdbcType="VARCHAR"/>
            <result property="chrNum" column="chr_num" jdbcType="VARCHAR"/>
            <result property="chrSource" column="chr_source" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        chr_id,chr_name,chr_num,
        chr_source
    </sql>
</mapper>
